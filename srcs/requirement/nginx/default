server {
    listen 443 ssl;
    listen [::]:443 ssl;

    ssl_protocols TLSv1.2 TLSv1.3;
    ssl_certificate /etc/ssl/certs/server.crt;
    ssl_certificate_key /etc/ssl/private/server.key;

    root /var/www/html;
    server_name _;
    index index.nginx-debian.html index.php index.htm;
    error_log /var/log/nginx/error.log;
